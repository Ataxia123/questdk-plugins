{
    "compilerOptions": {
      "rootDir": "src",
      "outDir": "dist",
      "strict": true,
      "target": "ES2021",
      "module": "commonjs",
      "sourceMap": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "skipLibCheck": true
    },
    "exclude": ["template", "dist"]
  }
  
  